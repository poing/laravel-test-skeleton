{
    "name": "poing/skeleton",
    "description": "Laravel Skeleton for Package Development",
    "require": {
        "illuminate/config": "^5.8",
        "illuminate/support": "^5.8",
        "php": "^7"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "laravel/framework": "^5.8"
    },
    "authors": [
        {
            "name": "Brian LaVallee",
            "email": "brian.lavallee@invite-comm.jp"
        }
    ],
    "autoload": {
        "psr-4": {
            "Poing\\Skeleton\\": "source"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Poing\\Skeleton\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Poing\\Skeleton\\SkeletonServiceProvider"
            ]
        }
    }

}
